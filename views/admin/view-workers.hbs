<section>
    <div class="container">
      <div class="row mt-3">
        
      </div>
        <table class="table mt-3">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Address</th>
      <th scope="col">Category</th>
      <th scope="col">Email</th>
      <th>Status</th>
      <th scope="col">Image</th>
    </tr>
  </thead>
  <tbody>
    {{#each workers}}
    <tr>
      <td>{{this.workerName}}</td>
      <td>{{this.address}}</td>
      <td>{{this.category}}</td>
      <td>{{this.email}}</td>
      <td>{{this.activeStatus}}</td>
      <td><img src="/workerImages/{{this._id}}.jpg" height="50" width="50" alt=""></td>
      <td><a href="/admin/worker-edit/{{this._id}}" class="btn btn-warning">EDIT</a></td>
      <td><a href="/admin/delete-worker-details/{{this._id}}" class="btn btn-danger" onclick="return confirm('Do you want to delete the Profile of {{this.workerName}}?')">DELETE</a></td>
    </tr>
    {{/each}}
  </tbody>
</table>
    </div>
</section>