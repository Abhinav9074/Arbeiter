<section>
    <div class="container">
      <div class="row mt-3">
      </div>
      <div class="alert alert-success" role="alert">
  The Untracked Bookings Will Be Displayed Here 
</div>
        <table class="table mt-3">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Address</th>
      <th scope="col">Phone</th>
    </tr>
  </thead>
  <tbody>
    {{#each workNotification}}
    <tr>
      <td>{{this.name}}</td>
      <td>{{this.address}}</td>
      <td>{{this.phone}}</td>
      <td><a href="/worker/showDetails/{{this.userId}}" class="btn btn-primary">VIEW MORE</button></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>
    </div>
</section>