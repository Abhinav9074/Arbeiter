<div class="container">
    <article class="card">
        <header class="card-header"> My Bookings / Tracking </header>
        <div class="card-body">
            <h6>Booking ID: {{trackData._id}}</h6>
            <article class="card">
                <div class="card-body row">
                    <div class="col"> <strong>Confirmed Date</strong> <br>{{trackData.day}}-{{trackData.month}}-{{trackData.year}}</div>
                    <div class="col"> <strong>Worker Name</strong> <br> {{trackData.workerName}} <i class="fa fa-phone"></i>
                        </div>
                    <div class="col"> <strong>Status:</strong> <br> {{trackData.status}} </div>
                    <div class="col"> <strong>Confirmed Time</strong> <br> {{trackData.hour}}:{{trackData.minutes}} </div>
                </div>
            </article>
            <div class="track">
                <div class="step active"> <span class="icon">✓<i class="fa fa-check"></i> </span> <span
                        class="text">Booking confirmed</span> </div>
                <div class="step"> <span class="icon"> {{#if trackData.userAuth}}✓{{/if}}<i class="fa fa-box"></i> </span> <span class="text">Work Completed</span></div>
            </div>
            <hr>
            <hr>
            {{#if trackData.userAuth}}

            {{else}}
            {{#if trackData.workStatus}}
            <a href="/userAuth/{{trackData._id}}" class="btn btn-warning" data-abc="true"> <i class="fa fa-chevron-left"></i> Work Completed</a>
            {{/if}}
            {{/if}}
        </div>
    </article>
</div>
{{#if trackData.userAuth}}
<section>
    <form action="/reviewSubmit/{{trackData._id}}" method="post">
    <h5>Write a review To appreciate our Worker</h5>
    <label for="category" class="form-label">Rating</label>
                        <select class="form-control" name="rating" id="category">
                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                            <option>4</option>
                            <option>5</option>
                            </select>
    <input class="box" type="textarea" name="review"><br>
    <button class="btn btn-success">Submit</button>
    <input  type="hidden" value="{{trackData.User_Name}}" name="userName"><br>
    <input  type="hidden" value="{{trackData.workerName}}" name="workerName"><br>
    <input  type="hidden" value="{{trackData.workerId}}" name="workerId"><br>
    <input  type="hidden" value="{{trackData.userId}}" name="userId"><br>
    <input  type="hidden" value="{{trackData.day}}" name="day"><br>
    <input  type="hidden" value="{{trackData.month}}" name="month"><br>
    <input  type="hidden" value="{{trackData.year}}" name="year"><br>
    
    </form>
</section>
{{/if}}

{{#if trackData.userAuth}}
