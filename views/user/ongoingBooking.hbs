<section>
    <div class="container">
      <div class="row mt-3">
      </div>
      <div class="alert alert-success" role="alert">
  All Accepted Bookings Will Be Displayed Here 
</div>
        <table class="table mt-3">
  <thead>
    <tr>
      <th scope="col">Worker Name</th>
      <th scope="col">Category</th>
      <th scope="col">Place</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>
    <tr>
        
      <td>{{confirmData.workerName}}</td>
      <td>{{confirmData.category}}</td>
      <td>{{confirmData.workerPlace}}</td>
      <td>{{confirmData.status}}</td>
      {{#if confirmData.status}}
      <td><a  href="/bookingDetails/{{confirmData._id}}">View More</a></td>
      {{/if}}
    </tr>
    
  </tbody>
</table>
    </div>
</section>